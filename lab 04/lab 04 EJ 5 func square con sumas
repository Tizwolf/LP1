
//Calcula el cuadrado de v y lo imprime

#include <iostream>
using namespace std;
int square(int v) //Declaracion de la funcion
{
	int i=0;
    int b=0;
    for(i=1;i<=v;i++)
       {
        b=b+v;
       }
	return b;
}


int main()
{
	for (int i=0; i<100; i++) 
	cout<<i<<'\t'<<square(i)<<'\n';
}
