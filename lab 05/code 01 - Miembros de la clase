//MIEMBROS DE LA CLASE
//code 01

#include <iostream>

using namespace std;

class X 
{
	public: 
		int m; 				//dato miembro
		int mf(int v){
			int old = m; 
			m = v;
			return old;		//función miembro
		}
};

int main()
{
	X var;					//var es una variable de tipo X
	var.m = 7;				//asigna valor al dato miembro m de var
	int x = var.mf(9);		//llama a la función miembro mf()
	return x; 
}
