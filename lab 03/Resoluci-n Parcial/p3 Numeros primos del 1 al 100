//EXAMEN PARCIAL
//PREGUNTA 3
//Hacer un programa que imprima los numeros primos del 1 al 100 


#include <iostream>
using namespace std;
int main (){
	for (int i=2;i<=100; i++){
		
		int aux = 0;
			for (int j=1; j<=100;j++){
			
				if ((i%j) == 0){
					aux++;
					}
			}
		
		if (aux==2){
			cout<<i<<endl;
			}
	}
	
}
