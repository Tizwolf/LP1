#include <iostream>
using namespace std;
int main()
{	
cout<<"Ingrese tres nombres o palabras"<<endl;
string x, y, z;
string menor, mayor, medio ;
cin>>x>>y>>z;
	
	if (y>x){
		
			mayor = y;
			menor = x;
		}
	else 
		mayor=x;
		menor=y;
		
	if (z>mayor){
		
			medio = mayor;
			mayor = z;
		}
	if (z<menor)
		{
			medio=menor;
			menor=z;
		}
	else 
		medio = z;

cout<<"La palabra mayor lexicograficamente es: "<<mayor<<endl;
cout<<"La palabra del medio lexicograficamente es:: "<<medio<<endl;
cout<<"La palabra menor lexicograficamente es:: "<<menor<<endl;
	
}
