#include <iostream>
#include <string>
using namespace std;
 
int main()
{
	cout<<"Ingrese su nombre y presione enter, got it?"<<endl;
	string nombre;
	int edad;
	cin>>nombre;
	cin>>edad;
	cout<<"Hola "<<nombre<<"("<<edad<<")"<<endl;
	return 0;

}


