//Ejercicio 02 : void to_lower(char* s)

#include <iostream>

using namespace std; 


void to_lower(char* s){
	
	for(int i= 0; s[i]; ++i){
		if (s[i]>='A' && s[i]<= 'Z')
			s[i] += 'a' - 'A';		
	}	
	    for (int i = 0; s[i]; ++i) {
        cout << s[i];
    }
}


int main()
{
	string s;
    cin>>s;
    to_lower(&s[0]);
}
