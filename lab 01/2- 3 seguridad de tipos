#include <iostream>
using namespace std;

int main()
{	
	
	unsigned int a=2000;
	unsigned char c= a;
	
	cout<<c<<endl;
	cout<<a<<endl;
	
	int b= c;
	
	if (a==b)
		cout<<"Wow tenemos caracteres iguales"<<endl;
	else
	cout<<a<<" Es diferente de "<<b<<endl;
	
	cout<<"El valor de c "<<c<<endl;
	cout<<"El valor de a "<<a<<endl;
}
